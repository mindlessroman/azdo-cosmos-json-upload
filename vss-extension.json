{
    "manifestVersion": 1,
    "id": "json-upload-cosmosdb",
    "publisher": "mindlessroman",
    "version": "0.2.0",
    "name": "Azure Cosmos DB Data Upload",
    "description": "A task to automate adding data upload to a Cosmos DB instance",
    "public": true,
    "categories": ["Azure Pipelines"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "vss-extension-icon.png"
    },
    "contributions": [
        {
            "id": "upload-json-cosmos-db",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "cosmosUploadTask"
            }
        }
    ],
    "files": [
        {
            "path": "cosmosUploadTask"
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/mindlessroman/azdo-cosmos-json-upload"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "links": {
        "support": {
            "uri": "https://github.com/mindlessroman/azdo-cosmos-json-upload/issues"
        },
        "license": {
            "uri": "https://github.com/mindlessroman/azdo-cosmos-json-upload/blob/master/LICENSE"
        }
    },
    "tags": ["cosmosdb", "azure", "azure cosmos", "cosmos db", "json upload"]
}
